{% extends 'base.html.twig' %}

{% block title %}AdresseFacturation index{% endblock %}

{% block body %}
    <h1>AdresseFacturation index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>UserId</th>
                <th>LibAdressFacturation</th>
                <th>CpAdresseFacturation</th>
                <th>VilleAdresseFacturation</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for adresse_facturation in adresse_facturations %}
            <tr>
                <td>{{ adresse_facturation.id }}</td>
                <td>{{ adresse_facturation.UserId }}</td>
                <td>{{ adresse_facturation.LibAdressFacturation }}</td>
                <td>{{ adresse_facturation.CpAdresseFacturation }}</td>
                <td>{{ adresse_facturation.VilleAdresseFacturation }}</td>
                <td>
                    <a href="{{ path('app_adresse_facturation_show', {'id': adresse_facturation.id}) }}">show</a>
                    <a href="{{ path('app_adresse_facturation_edit', {'id': adresse_facturation.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="6">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_adresse_facturation_new') }}">Create new</a>
{% endblock %}
